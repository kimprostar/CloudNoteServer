<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.itstudy.cloudnoteserver.domain">
	<class name="Userbuy" table="Userbuy">
		<composite-id>
			<key-property name="userid" column="userid" ></key-property>
			<key-property name="fileid" column="fileid"></key-property>
		</composite-id>
	</class>
</hibernate-mapping>